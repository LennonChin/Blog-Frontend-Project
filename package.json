{
  "name": "Blog-Front-Project",
  "version": "1.0.0",
  "description": "Web front for my blog",
  "author": "Lennon <i@coderap.com>",
  "private": true,
  "scripts": {
    "dev:client": "cross-env NODE_ENV=development && webpack-dev-server --config build/webpack.dev.conf.js",
    "dev:server": "nodemon server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.prod.conf.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.conf.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:dev:client": "cross-env NODE_ENV=development webpack --config build/webpack.prod.conf.js",
    "build:dev:server": "cross-env NODE_ENV=development webpack --config build/webpack.server.conf.js",
    "build:dev": "npm run clean && npm run build:dev:client && npm run build:dev:server",
    "start": "node server/server.js",
    "clean": "rimraf dist && rimraf server-build",
    "lint": "eslint --ext .js,.vue src",
    "upload": "node build/uploadAssets.js",
    "analyze": "cross-env npm_config_report=true npm run build:client"
  },
  "dependencies": {
